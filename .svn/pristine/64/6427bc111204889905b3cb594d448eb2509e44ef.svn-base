<%@taglib uri="/struts-tags" prefix="s"%>
<h1>Change Password</h1>
<table>
	<s:actionerror />
	<s:form action="changepassword">
		<tr>
			<td>New Password:</td>
			<td><s:textfield name="newPassword" value="%{username}" /></td>
		</tr>
		<tr>
			<td>Confirm Password:</td>
			<td><s:textfield name="confirmPassword" /></td>
		</tr>
		<tr>
			<td colspan="2" align="right"><s:submit value="OK" /> <s:submit
					value="Cancel" action="login" /></td>
		</tr>
	</s:form>
</table>
