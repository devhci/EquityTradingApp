log4j.rootLogger=INFO, CA

log4j.appender.CA=org.apache.log4j.ConsoleAppender
log4j.appender.CA.layout=org.apache.log4j.PatternLayout
log4j.appender.CA.layout.ConversionPattern=%d %-5p %c.%M:%L - %m%n

log4j.logger.org.apache.struts2=DEBUG
log4j.logger.com.opensymphony=DEBUG
log4j.logger.ognl=INFO
log4j.logger.org.apache.struts2.util.StrutsTypeConverter=DEBUG
log4j.logger.org.hibernate=INFO
log4j.logger.com.sapient.portfoliomanagement.dao=DEBUG
log4j.logger.org.apache.tiles=DEBUG
